event.dataset : "email" and
event.action : "received" and
email.direction : "inbound" and
email.url.domain : * and
email.url.total_count >= 1 and
not email.from.domain : (
  "yourcompany.com",
  "trustedvendor.com"
) and
email.subject : (
  "*invoice*" or "*payment*" or "*urgent*" or
  "*delivery*" or "*resume*" or "*password*" or "*confirm*"
) and
email.url.domain : (
  "*.site" or "*.shop" or "*.top" or "*.live" or "*.xyz" or "*.online" or "*.click" or "*.ru"
)
